{
  "manifest_version": 3,
  "name": "Mipa Tab Manager",
  "version": "1.0",
  "description": "标签页管理器，帮助您将浏览器标签页组织成集合。",
  "permissions": [
    "tabs",
    "storage"
  ],
  "host_permissions": [
    "https://api.github.com/*"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "js/background.js",
    "type": "module"
  }
}
